{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php",
      "config": {
        "maxLambdaSize": "15mb",
        "phpVersion": "8.1.10" 
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ],
  "env": {
    "APP_KEY": "@APP_KEY",
    "APP_URL": "APP_URL",
    "APP_ENV": "production",
    "APP_DEBUG": "0",
    "LOG_CHANNEL": "stderr",
    "CACHE_DRIVER": "array",
    "SESSION_DRIVER": "cookie",
    "QUEUE_CONNECTION": "sync",
    "VIEW_COMPILED_PATH": "/tmp/storage/framework/views"
 
  }
}